        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </BarChart>
      <ComboBox fx:id="boxDis" layoutX="45.0" layoutY="68.0" prefHeight="42.0" prefWidth="383.0" promptText="Disciplinas" />
